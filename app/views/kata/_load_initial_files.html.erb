
<script type="text/javascript">
'use strict';
$(() => {

  cd.loadInitialFiles = () => {
    const files = [];
    <% @files.each do |filename,file| %>
      var filename = "<%= j raw(filename) %>";
      var content = "<%= j raw(file['content']) %>";
      files.push({ name:filename, content:content });
    <% end %>
    cd.kata.editor.createFiles(files);
  };

});
</script>
