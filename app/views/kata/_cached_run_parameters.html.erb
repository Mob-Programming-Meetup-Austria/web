
<!-- Parameters needed by kata.run_tests() so it can  -->
<!-- avoid hitting the saver service except for       -->
<!-- saver.kata_ran_tests()                           -->

<input type="hidden"
       name="index"
       value="">

<input type="hidden"
       name="image_name"
       value="<%= @kata.manifest.image_name %>">

<input type="hidden"
       name="max_seconds"
       value="<%= @kata.manifest.max_seconds %>">

<input type="hidden"
       name="hidden_filenames"
       value='<%= raw @kata.manifest.hidden_filenames %>'>


<script type="text/javascript"><!--
'use strict';
$(() => {
  cd.setEventIndex = (value) => {
    $("input[name='index']").val(value);
  };
  cd.setEventIndex("<%= @kata.events.last.index %>");
});
//--></script>
