
<%= render partial: 'editor_tab_size' %>
<%= render partial: 'traffic_lights_scroller' %>

<div id="edit-top-row">
  <%= render partial: 'avatar_image' %>
  <%= render partial: 'test_button' %>
  <%= render partial: 'traffic_lights_count_total' %>
  <%= render partial: 'traffic_lights_count_predict' %>
  <%= render partial: 'traffic_lights' %>
</div>

<%= form_tag( { action: 'run_tests', id: @id } ) do %>
  <table>
    <tr class="valign-top">
      <td class="filenames-box">
        <%= render partial: 'file_new_rename_delete' %>
        <%= render partial: 'filenames' %>
      </td>
      <td>
        <%= render partial: 'tabs' %>
        <%= render partial: 'repl' %>

        <%= render partial: 'editor' %>
        <%= render partial: 'hidden_inputs' %>
      </td>
    </tr>
  </table>
<% end %>
